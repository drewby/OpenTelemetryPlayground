{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Ops": {
    "Logging": {
      "Enabled": true
    },
    "Health": {
      "Enabled": true,
      "ReadyPath": "/health/ready",
      "LivePath": "/health/live"
    },
    "Metrics": {
      "Enabled": true,
      "MetricsPath": "/metrics"
    },
    "Tracing": {
      "Enabled": true,
      "IncomingIgnore": "/metrics, /health"
    }
  },
  "AllowedHosts": "*"
}